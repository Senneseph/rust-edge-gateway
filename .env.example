# Rust Edge Gateway Environment Configuration
# Copy this file to .env and fill in your actual values

# DigitalOcean Configuration
DIGITALOCEAN_ACCESS_TOKEN=your_digitalocean_token_here

# Deployment Configuration
DEPLOY_SERVER_IP=your_server_ip_here
TARGET_DOMAIN=your-domain.com
DOCS_DOMAIN=docs.your-domain.com

# SSH Configuration
# Windows: $env:USERPROFILE\.ssh\your_key
# Linux/Mac: $HOME/.ssh/your_key
SSH_KEY=$env:USERPROFILE\.ssh\your_key_name

# Security Configuration
DEFAULT_ADMIN_PASSWORD=your_secure_password_here

# Optional: Docker Hub Configuration
DOCKER_HUB_USERNAME=your_dockerhub_username

